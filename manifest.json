{

  "manifest_version": 2,
  "name": "Siteflag",
  "version": "0.1",

  "description": "Lets you take notes for websites.",

  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": [
        "assets/jquery-3.4.1.min.js", 
        "src/database.js",
        "src/siteflag.js"
      ]
    }
  ],  
  "browser_action": {
    "default_icon": {
      "16": "icons/icon_16.png",
      "32": "icons/icon_32.png"
    }
  }

}
